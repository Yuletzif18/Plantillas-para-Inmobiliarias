# Variables de entorno de ejemplo para RentUP
# Copia este archivo como .env y configura tus valores reales

# URL base de la aplicación
NUXT_APP_BASE_URL=http://localhost:3000

# API base URL
NUXT_PUBLIC_API_BASE=http://localhost:3000/api

# Clave secreta para la aplicación
NUXT_SECRET_KEY=tu-clave-secreta-super-segura-aqui

# Base de datos
DATABASE_URL=postgresql://usuario:password@localhost:5432/rentup_db

# APIs de terceros
GOOGLE_MAPS_API_KEY=tu-google-maps-api-key
EMAIL_SERVICE_API_KEY=tu-email-service-api-key

# Configuración de redes sociales
FACEBOOK_APP_ID=tu-facebook-app-id
TWITTER_BEARER_TOKEN=tu-twitter-bearer-token

# Configuración de almacenamiento
AWS_ACCESS_KEY_ID=tu-aws-access-key
AWS_SECRET_ACCESS_KEY=tu-aws-secret-key
AWS_S3_BUCKET=rentup-storage
AWS_REGION=us-west-2

# Configuración de pagos
STRIPE_PUBLIC_KEY=pk_test_tu-stripe-public-key
STRIPE_SECRET_KEY=sk_test_tu-stripe-secret-key

# Configuración de SEO
NUXT_PUBLIC_SITE_NAME="RentUP - Portal Inmobiliario"
NUXT_PUBLIC_SITE_DESCRIPTION="Portal inmobiliario líder para compra, venta y alquiler de propiedades"
NUXT_PUBLIC_SITE_URL=https://tu-dominio.com

# Configuración de desarrollo
NODE_ENV=development
DEBUG=true

# Configuración de email
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=tu-email@gmail.com
SMTP_PASS=tu-password-de-app

# Analytics
GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
FACEBOOK_PIXEL_ID=tu-facebook-pixel-id

# Configuración de caché
REDIS_URL=redis://localhost:6379

# JWT
JWT_SECRET=tu-jwt-secret-muy-seguro
JWT_EXPIRES_IN=7d

# Configuración de logs
LOG_LEVEL=info
LOG_FILE=logs/app.log